<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FTC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/FaithTabernacleChurch.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/animate.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-toggleable-sm navbar-dark position-fixed">
            <div class="container">
                <a class="navbar-brand fs-2" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/images/ftc.jpg" style="width: 20px; height: 20px;" /> FTC</a>
                <button class="navbar-toggler" style="box-shadow: none; border: none" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-end">
                    <ul class="navbar-nav gap-3 text-uppercase">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Ministries</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Events</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Privacy">Contacts</a>
                        </li>
                    </ul>
                    @if (User.IsInRole(SD.Role_Admin))
                    {
                        <partial name="_LoginPartial" />
                    }
                </div>
            </div>
        </nav>

    </header>
    <div class="hero">
        <div class="container-fluid" style="padding: 0;">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

    <footer class="bg-dark text-light">
        <div class="container py-5">
            <div class="row">
                <div class="col-lg-2 offset-lg-1">
                    <div class="lc-block mb-4 wow fadeInUp">
                        <h4 style="color: #fbc531">WHO WE ARE</h4>
                    </div>
                    <!-- /lc-block -->
                    <div class="lc-block small wow fadeInRight">
                        <p>About Us</p>
                        <p> Ministries<br></p>
                        <p>Events</p>
                        <p>Contacts</p>
                    </div>
                    <!-- /lc-block -->
                </div>
                <div class="col-lg-2 offset-lg-1">
                    <div class="lc-block mb-4 wow fadeInUp">
                        <h4 style="color: #fbc531">INFO</h4>
                    </div>
                    <!-- /lc-block -->
                    <div class="lc-block small wow fadeInRight">
                        <p>Pastors</p>
                        <p>Careers</p>
                        <p>Community</p>
                        <p>Blog</p>
                        <p>Gallery</p>
                    </div>
                    <!-- /lc-block -->
                </div>
                <div class="col-lg-2 offset-lg-1">
                    <div class="lc-block mb-4 wow fadeInUp">
                        <h4 style="color: #fbc531">HELP</h4>
                    </div>
                    <!-- /lc-block -->
                    <div class="lc-block small wow fadeInRight">
                        <p>Donate Now</p>
                        <p>FAQ</p>
                        <p>Privacy Policy</p>
                    </div>
                    <!-- /lc-block -->
                </div>
                <div class="col-lg-2 offset-lg-1">
                    <div class="lc-block mb-4 wow fadeInUp">
                        <h4 style="color: #fbc531">CONTACT US</h4>
                    </div>
                    <!-- /lc-block -->
                    <div class="lc-block small wow fadeInRight">
                        <p>Placer Surigao Del Norte</p>
                        <p>Please write us at: <a href="mailto:faithtabernaclechurch@gmail.com"> faithtabernaclechurch@gmail.com</a></p>
                        <p>Call us: <a href="tel:+639055550000">+639 05 55 50 000</a></p>
                    </div>
                    <!-- /lc-block -->
                    <div class="lc-block py-2 wow bounceIn">
                        <a class="text-decoration-none" href="#">
